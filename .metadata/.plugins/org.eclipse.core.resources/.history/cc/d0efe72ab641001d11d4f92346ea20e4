package ar.edu.unlp.info.oo1.Solidos;

public class Esfera extends Pieza{
	int radio;

	public Esfera(int radio, String material, String color) {
		super(material , color);
		this.radio = radio;
	}

	public int getRadio() {
		return radio;
	}

	public void setRadio(int radio) {
		this.radio = radio;
	}
}
